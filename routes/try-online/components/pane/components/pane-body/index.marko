import app from '~/routes/try-online/app';

$ var focused = input.focused;
$ var active = input.active;
$ var file = input.file;

<div.pane-body class=[focused && 'focused', active && 'active']>
    <if(file.preview)>
        <preview file=file no-update key='preview-'+file.path />
    </if>
    <else>
        <editor file=file no-update key='editor-'+file.path />
    </else>
</div>